{
  "name": "sohophp/schema-org",
  "description": "schema.org php library",
  "type": "library",
  "license": "MIT",
  "keywords": ["schema.org", "structural data"],
  "authors": [
    {
      "name": "sohophp",
      "email": "sohophp@gmail.com"
    }
  ],
  "homepage": "https://sohophp.com/post/read/7",
  "require": {
    "php": ">=7.2.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "symfony/console": "^4.1",
    "phpunit/phpunit": "^7.0",
    "symfony/templating": "^4.3@dev",
    "twig/twig": "2.0",
    "doctrine/inflector": "^2.0@dev",
    "friendsofphp/php-cs-fixer": "^2.14@dev"
  },
  "autoload": {
    "psr-4": {
      "Sohophp\\SchemaOrg\\": "src\\"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sohophp\\SchemaOrg\\Tests\\": "tests\\",
      "Sohophp\\SchemaOrg\\Examples\\": "examples\\"
    }
  },
  "bin": ["bin/schema"],
  "minimum-stability": "dev",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
